generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
  Room     Room[]
}

model Room {
  id     Int    @id @default(autoincrement())
  roomId String
  userId Int
  user   User   @relation(fields: [userId], references: [id])
}